        height:50px;
        float:right;
        margin:10px;
        text-align:center;
        line-height:50px;
    }
    .footer{
         margin:10px;
         border:solid 1px #999;
         position:absolute;
    }
    .appWH .footer-icon-holder{
        float: left;
        display: block;
        text-align: center;       
    }
    .ui-header .ui-btn-left{
        border:none !important;
    }
    .appWH .footer-icon-holder .ui-btn-corner-all{
        -moz-border-radius: 0 !important;
        -webkit-border-radius: 0!important;
        border-radius: 0!important;
    }
    .appWH .ui-btn-corner-all{
        -moz-border-radius: 5px !important;
        -webkit-border-radius: 6px!important;
        border-radius: 5px !important;
    }
    .footer-icon-holder .ui-btn-inner{    
        padding: 7px 38.8px;    
    }
    .appWH .footer-icon-holder img{
        width: 50px;
        height: 50px;
        display: block;
    }
    .appWH .footer-icon-holder label{
        text-shadow: none;    
    }    
    .left{
        float: left;
    }
    .right{
        float: right;    
    }
    .clear{
        clear: both;
    }    
    .footMid{
        height:100px;
    }
    .centerbox-outer{
        margin: 0px auto;
        float: left;       
        position: relative;
    }
    .centerbox-inner{
        float: left;
        position: relative;
    }
    .centerbox-inner a{
        color:#999 !important;
        text-decoration: none;
        font-size:14px !important;
    }
    #loginButton .ui-btn-corner-all {
        border-radius:12px !important;
    }
    .active-footer{
        background: rgba(0, 0, 0, 0.5) !important
